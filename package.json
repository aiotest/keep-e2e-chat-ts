{
  "name": "feathers-chat",
  "description": "A FeathersJS real-time chat application written in TypeScript",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "David Luecke",
    "email": "daff@neyeon.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^11.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run coverage",
    "dev": "ts-node-dev --no-notify src/",
    "start": "node lib/",
    "clean": "shx rm -rf test/data/",
    "coverage": "nyc npm run mocha",
    "mocha": "npm run clean && NODE_ENV=test mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc",
    "update-dependencies": "ncu -u"
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.11",
    "@feathersjs/authentication-local": "^5.0.11",
    "@feathersjs/authentication-oauth": "^5.0.11",
    "@feathersjs/configuration": "^5.0.11",
    "@feathersjs/errors": "^5.0.11",
    "@feathersjs/express": "^5.0.11",
    "@feathersjs/feathers": "^5.0.11",
    "@feathersjs/socketio": "^5.0.11",
    "@seald-io/nedb": "^4.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-nedb": "^6.0.0",
    "helmet": "^7.0.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/mocha": "^10.0.3",
    "@types/serve-favicon": "^2.5.6",
    "axios": "^1.6.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "npm-check-updates": "^16.14.6",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  }
}
